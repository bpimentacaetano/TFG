package Converter;

import Model.Contrato;
import java.util.HashMap;
import java.util.Map;

/**
 *
 * @author BÃ¡rbara
 */
public class ContratoConverter {
     public Map<String, Object> converterToMap(Contrato contrato) {
        Map<String, Object> mapContrato = new HashMap<String, Object>();

        mapContrato.put("id_compra", contrato.getId_compra());
        mapContrato.put("cnpj_contratada", contrato.getCnpj_contratada());
        mapContrato.put("cpf_contratada", contrato.getCpf_contratada());
        mapContrato.put("data_assinatura", contrato.getData_assinatura());
        mapContrato.put("data_inicio_vigencia", contrato.getData_inicio_vigencia());
        mapContrato.put("data_termino_vigencia", contrato.getData_termino_vigencia());
        mapContrato.put("fundamento_legal", contrato.getFundamento_legal());
        mapContrato.put("identificador", contrato.getIdentificador());
        mapContrato.put("licitacao_associada", contrato.getLicitacao_associada());
        mapContrato.put("modalidade_licitacao", contrato.getModalidade_licitacao());
        mapContrato.put("numero", contrato.getNumero());
        mapContrato.put("numero_aditivo", contrato.getNumero_aditivo());
        mapContrato.put("numero_licitacao", contrato.getNumero_licitacao());
        mapContrato.put("numero_processo", contrato.getNumero_processo());
        mapContrato.put("objeto", contrato.getObjeto());
        mapContrato.put("origem_licitacao", contrato.getOrigem_licitacao());
        mapContrato.put("tipo_contrato", contrato.getTipo_contrato());
        mapContrato.put("uasg", contrato.getUasg());
        mapContrato.put("vl_inicial", contrato.getValor_inicial());

        return mapContrato;
    }
}
